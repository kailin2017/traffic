<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@+id/navigation_realtime">

    <fragment
        android:id="@+id/navigation_realtime"
        android:name="com.kailin.traffic.ui.main.home.HomeFragment"
        android:label="@string/navigation_realtime"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_info"
        android:name="com.kailin.traffic.ui.main.dashboard.DashboardFragment"
        android:label="@string/navigation_info"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.kailin.traffic.ui.main.notifications.NotificationsFragment"
        android:label="@string/navigation_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>